<?xml version="1.0" encoding="utf-8"?><!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.example.Users" table="users">
        <meta attribute="class-description">This class
            contains developer's details.
        </meta>
        <id name="userId" type="integer" column="user_id">
            <generator class="native"/>
        </id>
        <property name="login" column="login" type="text"/>
        <property name="password" column="password" type="text"/>
        <property name="access" column="access" type="integer"/>
        <property name="status" column="status" type="integer"/>

    </class>


</hibernate-mapping>